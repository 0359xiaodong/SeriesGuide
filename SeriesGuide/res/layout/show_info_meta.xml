<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android">
    <TextView
        android:id="@+id/TextViewShowInfoAirtime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5sp"
        android:text="airs Tue 9:00 PM" />
    <TextView
        android:text="on Very Long Network Name (VLNM)"
        android:layout_marginLeft="5sp"
        android:id="@+id/TextViewShowInfoNetwork"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"></TextView>
    <TextView
        android:text="Continuing"
        android:layout_marginBottom="2sp"
        android:layout_marginLeft="5sp"
        android:id="@+id/TextViewShowInfoStatus"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"></TextView>
</merge>
